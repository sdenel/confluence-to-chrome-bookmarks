{% macro generate_page_subtree(page) -%}
<DT><A HREF="{{ confluence_base_url }}/pages/viewpage.action?pageId={{ page.id }}" ICON="data:image/png;base64,">{{ page.title }}</A>
{% if page.children | length > 0 %}
<DT><H3>{{ page.title }} - subpages ({{ page.children | length }})</H3>
<DL><p>
{% for child in page.children %}
{{ generate_page_subtree(child) }}
{% endfor %}
</DL><p>
{%- endif %}
{%- endmacro -%}
<!DOCTYPE NETSCAPE-Bookmark-file-1>
<META HTTP-EQUIV="Content-Type" CONTENT="text/html; charset=UTF-8">
<TITLE>Generated bookmarks</TITLE>
<H1>Generated bookmarks</H1>
{{ generate_page_subtree(page) }}